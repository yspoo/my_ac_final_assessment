<h1>List of Users</h1>
<ol>
  <%= @users.each do |user| %>
    <li><%= link_to user.username, user_path(user) %></li>
  <% end %>
</ol>

<h1>List of Notes of Users</h1>
<%= link_to "Create Note", new_note_path %>
<ul>
  <% @notes.each do |note| %>
  <li><%= link_to note.title %> by <%= link_to note.user.username, user_path(current_user) %></li>
  <%= link_to note.body %>
    <%= f.button :submit %>
  <% end %>
</ul>

